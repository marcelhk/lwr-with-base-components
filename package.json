{
  "name": "lwr-project-with-base-components",
  "version": "0.0.1",
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "rm -rf __lwr_cache__",
    "dev": "lwr serve",
    "start": "lwr serve --mode prod",
    "start:compat": "lwr serve --mode compat",
    "start:prod-compat": "lwr serve --mode prod-compat",
    "build": "lwr build --outputDir \"dist\" --mode prod",
    "build:compat": "lwr build --outputDir \"dist\" --mode compat",
    "build:prod-compat": "lwr build --outputDir \"dist\" --mode prod-compat"
  },
  "dependencies": {
    "@salesforce-ux/design-system": "^2.18.1",
    "lightning-base-components": "^1.15.2-alpha",
    "lwc": "^2.19.0",
    "lwr": "^0.7.1"
  },
  "engines": {
    "node": ">=14.15.4 <15"
  }
}
